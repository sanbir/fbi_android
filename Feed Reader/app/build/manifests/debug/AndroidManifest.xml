<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.javatechig.feedreader">

    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.javatechig.feedreader.MyApplication" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.javatechig.feedreader.FeedListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.javatechig.feedreader.FeedDetailsActivity">
        </activity>
        <activity android:label="@string/app_name" android:name="com.javatechig.feedreader.WebViewActivity">
        </activity>
    </application>
</manifest>
